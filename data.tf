
# # ---------------------------------------------------------------------------------------------------------------------#
# Get ssh public key for manager
# # ---------------------------------------------------------------------------------------------------------------------#
data "digitalocean_ssh_keys" "manager" {
  filter {
    key    = "name"
    values = ["manager"]
  }
}
# # ---------------------------------------------------------------------------------------------------------------------#
# Get ssh public key for admin
# # ---------------------------------------------------------------------------------------------------------------------#
data "digitalocean_ssh_keys" "admin" {
  filter {
    key    = "name"
    values = ["admin"]
  }
}
# # ---------------------------------------------------------------------------------------------------------------------#
# Get kubernetes latest version
# # ---------------------------------------------------------------------------------------------------------------------#
data "digitalocean_kubernetes_versions" "get" {
  version_prefix = "1.22.8-do.1"
}
